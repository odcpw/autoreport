<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui">
  <ribbon>
    <tabs>
      <tab id="tabAutoBericht" label="AutoBericht">
        <group id="grpAutoBerichtMain" label="AutoBericht">
          <button id="btnAutoBerichtImportChapter" label="Import chapter…" size="large" imageMso="TableInsert" onAction="AB_ImportChapterDialog" />
          <button id="btnAutoBerichtImportAll" label="Import all" size="large" imageMso="TableInsertDialog" onAction="AB_ImportAll" />
          <button id="btnAutoBerichtMarkdownChapter" label="Markdown chapter…" size="large" imageMso="TextToTable" onAction="AB_ConvertMarkdownChapter" />
          <button id="btnAutoBerichtMarkdownAll" label="Markdown all" size="large" imageMso="TextToTableDialog" onAction="AB_ConvertMarkdownAll" />
          <button id="btnAutoBerichtLogo" label="Insert logo" size="large" imageMso="PictureInsertFromFile" onAction="AB_InsertLogo" />
        </group>
        <group id="grpAutoBerichtPpt" label="PowerPoint">
          <button id="btnAutoBerichtPptReport" label="PPT Bericht" size="large" imageMso="SlideNew" onAction="AB_ExportPptReport" />
          <button id="btnAutoBerichtPptTraining" label="VG Seminar D" size="large" imageMso="Presentation" onAction="AB_ExportPptTraining" />
          <button id="btnAutoBerichtPptTrainingF" label="VG Seminar F" size="large" imageMso="Presentation" onAction="AB_ExportPptTrainingF" />
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>
